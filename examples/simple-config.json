{
  "projectId": "prj_...",
  "teamId": "team_...",
  "rules": [
    {
      "name": "Block Bad IPs",
      "description": "Block known malicious IP addresses",
      "conditionGroup": [
        {
          "conditions": [
            {
              "type": "ip_address",
              "op": "eq",
              "value": "192.168.1.1"
            }
          ]
        },
        {
          "conditions": [
            {
              "type": "ip_address",
              "op": "inc",
              "value": "10.0.0.0/24"
            }
          ]
        }
      ],
      "action": {
        "mitigate": {
          "action": "deny"
        }
      },
      "active": true
    },
    {
      "name": "Block ASNs",
      "description": "Block traffic from specific autonomous systems",
      "conditionGroup": [
        {
          "conditions": [
            {
              "type": "geo_as_number",
              "op": "eq",
              "value": "12345"
            }
          ]
        },
        {
          "conditions": [
            {
              "type": "geo_as_number",
              "op": "eq",
              "value": "67890"
            }
          ]
        }
      ],
      "action": {
        "mitigate": {
          "action": "deny"
        }
      },
      "active": true
    },
    {
      "name": "Challenge API Requests",
      "description": "Challenge requests to API endpoints without session cookie",
      "conditionGroup": [
        {
          "conditions": [
            {
              "type": "path",
              "op": "pre",
              "value": "/api"
            }
          ]
        }
      ],
      "action": {
        "mitigate": {
          "action": "challenge"
        }
      },
      "active": true
    },
    {
      "name": "Block No Cookie",
      "description": "Block requests without specific cookie",
      "conditionGroup": [
        {
          "conditions": [
            {
              "type": "cookie",
              "op": "nex",
              "key": "_session",
              "value": ""
            }
          ]
        }
      ],
      "action": {
        "mitigate": {
          "action": "deny"
        }
      },
      "active": true
    }
  ]
}
